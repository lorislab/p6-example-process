{
  "processId": "org.lorislab.p6.example.Test1",
  "processVersion": "1.0.0",
  "nodes": [
    {
      "name": "start",
      "nodeType": "START_EVENT",
      "sequence": {
        "to": [
          "service1"
        ]
      }
    },
    {
      "name": "service1",
      "nodeType": "SERVICE_TASK",
      "sequence": {
        "from": [
          "start"
        ],
        "to": [
          "gateway1"
        ]
      }
    },
    {
      "name": "gateway1",
      "nodeType": "PARALLEL_GATEWAY",
      "sequence": {
        "from": [
          "service1"
        ],
        "to": [
          "service3",
          "service4"
        ]
      },
      "sequenceFlow": "DIVERGING"
    },
    {
      "name": "service3",
      "nodeType": "SERVICE_TASK",
      "sequence": {
        "from": [
          "gateway1"
        ],
        "to": [
          "gateway2"
        ]
      }
    },
    {
      "name": "service4",
      "nodeType": "SERVICE_TASK",
      "sequence": {
        "from": [
          "gateway1"
        ],
        "to": [
          "gateway2"
        ]
      }
    },
    {
      "name": "gateway2",
      "nodeType": "PARALLEL_GATEWAY",
      "sequence": {
        "from": [
          "service3",
          "service4"
        ],
        "to": [
          "end"
        ]
      },
      "sequenceFlow": "CONVERGING"
    },
    {
      "name": "end",
      "nodeType": "END_EVENT",
      "sequence": {
        "from": [
          "gateway2"
        ]
      }
    }
  ]
}